  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/itw-creative-works/web-manager/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "backup": {
    "dependencies": {
      "@sentry/browser": "^6.19.7",
      "cookieconsent": "^3.1.1",
      "firebase": "^8.10.1",
      "lazysizes": "^5.3.2"
    }
  },
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "@sentry/browser": "^10.15.0",
    "firebase": "^12.3.0",
    "itwcw-package-analytics": "^1.0.6",
